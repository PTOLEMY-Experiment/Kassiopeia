		<!-- fence -->
		<extruded_circle_surface name="fence_shape" zmin="-0.1" zmax="0.1" extruded_mesh_count="100" extruded_mesh_power="1">
			<circle x="0." y="0." radius="0.15" circle_mesh_count="128"/>
