		</tag>
		<tag name="bcenter_plate">
			<surface name="bcenter_plate_surface" node="bcenter_plate_shape"/>
		</tag>

	</space>

	<space name="world" node="cylinder_space">
		<space name="fence_space" tree="fence_assembly"/>
		<space name="electrode_space" tree="electrode_assembly">
			<transformation rotation_euler="0. 90. 0."/>
		</space>
	</space>

	<appearance name="app_electrode" color="123 123 255 127" arc="72" surfaces="world/electrode_space/@tcenter_plate"/>
