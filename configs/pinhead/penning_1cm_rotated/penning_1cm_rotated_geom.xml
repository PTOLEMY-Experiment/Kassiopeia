<!-- 	<appearance name="app_electrode" color="  0   0 128 127" arc="72" surfaces="world/electrode_space/@bbumper_plate1"/>
	<appearance name="app_electrode" color="  0   0 128 127" arc="72" surfaces="world/electrode_space/@bbumper_plate2"/> -->
