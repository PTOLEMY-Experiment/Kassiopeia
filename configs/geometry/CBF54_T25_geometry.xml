				</tag>


				<tag name="tr_x0_negy_[i]">
					<surface name="tr_x0_negy_[i]_surface" node="filter_electrode_surface_neg_y_x0">
						<transformation displacement="0. 0. {-([i]+1)*[festep]}"/>
					</surface>
				</tag>
			</loop>

		</tag>
	</space>


